{
    "rewrites": [
        {
            "source": "/api/proxy-send",
            "destination": "https://mailservice-nine.vercel.app/api/send"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)\\.(.*)",
            "destination": "/$1.$2"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}